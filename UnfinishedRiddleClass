import java.util.Scanner;

@SuppressWarnings("unused")

public class UnfinishedRiddleClass {

	public static void main(String[] args) {
		System.out.println("Riddle! Do you except? Yea or Nay");
		String noInput = "Nay";
		String yeInput = "Yea";
		Scanner sca = new Scanner(System.in);
		String userInput = sca.nextLine();
		
		sca.close();
		
		if (userInput.equalsIgnoreCase(noInput)){
			   System.out.println("You said no :(");
		} else if (userInput.equalsIgnoreCase(yeInput)){
			System.out.println("You said yes!");
			 
		} else {
			System.out.println("Sorry, Unfortionetly i'm a dumb computer, I can't understand you");
		} 
		
	}

}
